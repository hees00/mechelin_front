<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent">
    <ImageView
        android:id="@+id/noncomplete_icon_iv"
        android:layout_width="40dp"
        android:layout_height="45dp"
        android:src="@drawable/map_serviceworkingon_run_xxhdpi"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/noncomplete_info_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="열심히 준비중인 서비스입니다."
        android:fontFamily="@font/scdream4"
        android:textSize="14dp"
        android:textColor="@color/main_color"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/noncomplete_icon_iv"/>

    <TextView
        android:id="@+id/noncomplete_info2_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="조금만 기다려주세요:)"
        android:fontFamily="@font/scdream4"
        android:textSize="10dp"
        android:textColor="@color/mechelin_gray_font2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/noncomplete_info_tv"/>

    </androidx.constraintlayout.widget.ConstraintLayout>




</androidx.constraintlayout.widget.ConstraintLayout>